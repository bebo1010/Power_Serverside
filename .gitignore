/vendor/autoload.php
/vendor/composer/autoload.php

# Ignore any backup files
*.bak
*.swp

# appserv data
/appserv/*
/cgi-bin/*
/phpMyAdmin/*
.htaccess

# default php files
index.php
phpinfo.php
