# Ignore vendor dependencies installed via Composer
/vendor/

# Ignore any log files
*.log

# Ignore IDE-specific files and directories
.vscode/
.idea/

# Ignore build artifacts
/build/

# Ignore user-specific environment files
.env
.env.local

# Ignore session and cache files
/var/sessions/
/var/cache/

